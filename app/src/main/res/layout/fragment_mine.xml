<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.fragment.MineFragment">

    <include android:id="@+id/mine_alarm"
        layout="@layout/layout_mine_alarm"/>
    <include
        android:id="@+id/mine_language"
        layout="@layout/layout_mine_item"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@id/mine_alarm"
        android:layout_marginTop="25dp"
        />
    <include
        android:id="@+id/mine_feedback"
        layout="@layout/layout_mine_item"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@id/mine_language"/>
    <include
        android:id="@+id/mine_privacy"
        layout="@layout/layout_mine_item"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@id/mine_feedback"/>
    <include
        android:id="@+id/mine_shared"
        layout="@layout/layout_mine_item"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@id/mine_privacy"/>
    <include
        android:id="@+id/upgrade"
        layout="@layout/layout_mine_item"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@id/mine_shared"/>

</androidx.constraintlayout.widget.ConstraintLayout>